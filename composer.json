{
  "name": "drupal/generateur-sites",
  "version": "0.1.0",
  "description": "Générateur de site drupal Perso",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Matthieu girardeau",
      "role": "Product manager"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "path",
      "url": "project/modules/atolcd/*"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/colorbutton",
        "version": "4.15.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.15.0.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/colordialog",
        "version": "4.15.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/colordialog/releases/colordialog_4.15.0.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/panelbutton",
        "version": "4.15.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.15.0.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/indent",
        "version": "4.15.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/indent/releases/indent_4.15.0.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/indentblock",
        "version": "4.15.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/indentblock/releases/indentblock_4.15.0.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "ckeditor-plugin/colorbutton": "4.15.0",
    "ckeditor-plugin/colordialog": "4.15.0",
    "ckeditor-plugin/indent": "4.15.0",
    "ckeditor-plugin/indentblock": "4.15.0",
    "ckeditor-plugin/panelbutton": "4.15.0",
    "drupal/address": "^1.8",
    "drupal/admin_toolbar": "^2.3",
    "drupal/build_hooks": "2.3",
    "drupal/ckeditor_config": "3.0",
    "drupal/ckeditor_indentblock": "*",
    "drupal/colorbutton": "1.2",
    "drupal/colordialog": "^1.3",
    "drupal/config_ignore": "^2.2",
    "drupal/console": "^1.9",
    "drupal/content_sync": "^2.1",
    "drupal/ctools": "^3.2",
    "drupal/datagouvfr_geocoder": "^1.1",
    "drupal/devel": "^2.1",
    "drupal/geocoder": "^2.21",
    "drupal/geofield": "^1.15",
    "drupal/imce": "^2.2",
    "drupal/imce_rename_plugin": "^1.4",
    "drupal/jsonapi_permission": "^1.0",
    "drupal/pathauto": "^1.6",
    "drupal/role_hierarchy": "^2.1",
    "drupal/simple_menu_permissions": "^1.3",
    "drupal/sitemap": "^1.5",
    "drupal/svg_image": "^1.9",
    "drupal/taxonomy_menu_ui": "^1.1",
    "drupal/token": "^1.5",
    "drupal/weight": "^3.1",
    "drush/drush": "*",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/core-composer-scaffold": "^9.0",
    "drupal/core-recommended": "^9.0",
    "vlucas/phpdotenv": "^5.1",
    "webflo/drupal-finder": "^1.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "vendor-dir": "html/vendor",
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "fix-right-on-site": "chmod u+w -R html/sites/default || true",
    "pre-install-cmd": [
      "SiteGen\\composer\\ScriptHandler::checkComposerVersion",
      "@fix-right-on-site"
    ],
    "pre-update-cmd": [
      "SiteGen\\composer\\ScriptHandler::checkComposerVersion",
      "@fix-right-on-site"
    ],
    "post-install-cmd": [
      "@drupal-scaffold",
      "SiteGen\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "@drupal-scaffold",
      "SiteGen\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core": {
        "2915792 - Fix menu link view permissions": "patchs/menu_link_content-view-permissions.patch",
        "2996114 - Internal check ressource type of routes": "patchs/routes-internal-check.patch",
        "Require global_settings.php first when in multsite": "patchs/multisite_share_global_settings.patch",
        "2738051 - Some entities can be NULL": "patchs/filter-out-nulls.patch",
        "2857304 - Multisite setup based on paths is broken": "patchs/drupal-multisites-subdirectories.patch"
      },
      "drupal/content_sync": {
        "3052772 - Entity manager implemented during import": "https://www.drupal.org/files/issues/2019-05-06/content_sync-entity_manager_refs.patch",
        "2873074#comment-13019230 - Drush commands": "patchs/content-sync-drush-commands.patch",
        "2982986 - Installation causes error (v2)": "https://www.drupal.org/files/issues/2019-03-13/content_sync-Installation_causes_error_v2-2982986-10-D8.patch",
        "3076902 - Import content of type .": "https://www.drupal.org/files/issues/2019-12-11/Error-importing-content-3076902-2.patch",
        "3056550 - Warning: Parameter must be an array or a Countable object": "https://www.drupal.org/files/issues/2019-05-23/content_sync_fix_notice-3056550-2.patch"
      }
    },
    "installer-types": [
      "drupal-default-module"
    ],
    "installer-paths": {
      "html/core": [
        "type:drupal-core"
      ],
      "html/libraries/{$name}": [
        "type:drupal-library"
      ],
      "html/libraries/ckeditor/plugins/{$name}": [
        "vendor:ckeditor-plugin"
      ],
      "html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "html/drush/Commands/{$name}": [
        "type:drupal-drush"
      ],
      "html/console/{$name}": [
        "type:drupal-console-language"
      ],
      "html/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "html/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "html/",
        "project-root": "project/"
      },
      "symlink": true,
      "overwrite": true,
      "file-mapping": {
        "[project-root]/.gitattributes": false,
        "[project-root]/.editorconfig": false,
        "[web-root]/robots.txt": "assets/robots-default.txt",
        "[web-root]/.eslintrc.json": false,
        "[web-root]/.eslintignore": false,
        "[web-root]/sites/default/settings.php": "project/conf/settings.php"
      }
    }
  }
}
